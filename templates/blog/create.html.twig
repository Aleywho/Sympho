{% extends 'base.html.twig' %}

{% block body %}
    <h1> Création d'article</h1>

    {{ form_start(formBillet) }}

    <div class="form-group">
        <label for="">Prénom</label>
        {{ form_widget(formBillet.prenom) }}
    </div>
    <div class="form-group">
        <label for="">Nom</label>
        {{ form_widget(formBillet.nom) }}
    </div>
    <div class="form-group">
        <label for="">Date de naissance</label>
        {{ form_widget(formBillet.datedenaissance) }}
    </div>
    {{ form_end(formBillet) }}

{% endblock %}
